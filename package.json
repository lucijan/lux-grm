{
  "name": "lux-grm",
  "displayName": "LuX-GRM",
  "description": "Luci's GRM Extension",
  "version": "0.0.1",
  "publisher": "lucijan",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucijan/lux-grm.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "lux-grm.insertDebugInclude",
      "title": "Insert debug.h include"
    }, {
      "command": "lux-grm.createClass",
      "title": "Create New Class..."
    }],
    "menus": {
      "explorer/context": [{
        "command": "lux-grm.createClass",
        "when": "explorerResourceIsFolder",
        "group": "navigation"
        }]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
